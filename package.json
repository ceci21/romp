    "test": "cd client && react-scripts test --env=jsdom",
    "eject": "cd client && react-scripts eject"
  },
  "dependencies": {
    "express": "^4.13.3",
    "mongoose": "^4.13.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "^1.0.17",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "react-scripts": "1.0.17"
  },
  "engines": {
    "node": "6.10.0"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "contributors": [
    "Cameron Malloy",
    "Cecilia Goss",
    "Clay Smart"
  ]
