{
  "name": "romp",
  "version": "1.0.0",
  "description": "An app for gamers to meet up and play.",
  "main": "server/index.js",
  "author": "colinear",
  "scripts": {
    "start": "cd client && react-scripts build && react-scripts start && cd ../server && node index.js",
    "client": "cd client && react-scripts start",
    "server": "nodemon server/index.js",
